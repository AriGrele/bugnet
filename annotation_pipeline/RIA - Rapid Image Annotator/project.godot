config_version=4

[application]

config/name="Rapid Image Annotator"
config/description="A graphical user interface for image annotation, designed to allow rapid text and point labeling of images with minimal load times."
run/main_scene="res://code/Main.tscn"
boot_splash/image="res://assets/large icon.png"
config/icon="res://assets/small icon.png"
config/windows_native_icon="res://assets/icon_dark.ico"

[debug]

settings/crash_handler/message="Please include this message when reporting the crash error"

[display]

window/size/width=741
window/size/height=512
window/dpi/allow_hidpi=true
window/energy_saving/keep_screen_on=false
window/stretch/mode="2d"
window/stretch/aspect="expand"

